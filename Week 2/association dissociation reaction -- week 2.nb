Cell[BoxData[{
 RowBox[{
  RowBox[{"odes", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{"s", "'"}], "[", "t", "]"}], "==", 
      RowBox[{"-", "v"}]}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"r", "'"}], "[", "t", "]"}], "==", 
      RowBox[{"-", "v"}]}], ",", 
     RowBox[{
      RowBox[{
       RowBox[{"sr", "'"}], "[", "t", "]"}], "==", "v"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"rateeqs", "=", 
   RowBox[{"v", "->", 
    RowBox[{
     RowBox[{"kf", " ", 
      RowBox[{"s", "[", "t", "]"}], " ", 
      RowBox[{"r", "[", "t", "]"}]}], "-", 
     RowBox[{"kb", " ", 
      RowBox[{"sr", "[", "t", "]"}]}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"param", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"kf", "->", "25"}], ",", 
     RowBox[{"kb", "->", "1"}]}], "}"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"initcond", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{
      RowBox[{"s", "[", "0", "]"}], "==", "2"}], ",", 
     RowBox[{
      RowBox[{"r", "[", "0", "]"}], "==", "1"}], ",", 
     RowBox[{
      RowBox[{"sr", "[", "0", "]"}], "==", "0"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"tsol", "=", 
   RowBox[{"NDSolve", "[", 
    RowBox[{
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"odes", "/.", "rateeqs"}], "/.", "param"}], ",", "initcond"}],
       "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"s", ",", "r", ",", "sr"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"t", ",", "0", ",", "100"}], "}"}]}], "]"}]}], ";"}]}], "Input",\

